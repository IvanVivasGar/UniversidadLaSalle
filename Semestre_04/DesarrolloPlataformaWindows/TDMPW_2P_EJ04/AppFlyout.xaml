<?xml version="1.0" encoding="utf-8" ?>
<FlyoutPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:local = "clr-namespace:TDMPW_2P_EJ04"
             x:Class="TDMPW_2P_EJ04.AppFlyout"
             Title="AppFlyout">

        <FlyoutPage.Flyout>
            <local:FlyoutMenuPage
                x:Name = "flyoutPage"/>
        </FlyoutPage.Flyout>

        <FlyoutPage.Detail>
            <NavigationPage>
                <x:Arguments>
                    <local:MainPage/>
                </x:Arguments>
            </NavigationPage>
        </FlyoutPage.Detail>
</FlyoutPage>